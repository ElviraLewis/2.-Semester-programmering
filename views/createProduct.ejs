    <div class="createProduct">
      <div class="form">
        <form id="product-form">
          <input type="text" placeholder="Product name" id="product_name" />
          <input type="number" placeholder="Pris" id="price" />
          <input required type="url" name="image" placeholder="https://google.com/myimage.png">
          <label for="categories">Category :</label>
          <select name="categories" id="productCategory">
            <option value="Portvin">Portvin</option>
            <option value="Gin">Gin</option>
            <option value="Vodka">Vodka</option>
            <option value="Vin">Vin</option>
            <option value="Andet">Andet</option>
          </select>
          <input value="Submit" type="submit" />
        </form>
      </div>
